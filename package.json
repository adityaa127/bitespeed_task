{
  "name": "bitespeed-identify-service",
  "version": "1.0.0",
  "description": "Bitespeed /identify endpoint implementation with Node.js, TypeScript, Express, and Prisma/Postgres.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "vercel-build": "npx prisma generate && npx prisma migrate deploy && npm run build",
    "test:smoke": "./test-identify.sh",
    "test:integration": "npm run build && node --test dist/tests/**/*.integration.test.js"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "express": "^4.22.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.13",
    "prisma": "^5.21.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
